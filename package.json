{
  "name": "fileshare",
  "version": "1.0.0",
  "description": "Anonymous FileShare MERN App",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "install-client": "cd client && npm ci --include=dev",
    "install-server": "cd server && npm install",
    "install-all": "npm ci && npm run install-client && npm run install-server",
    "build": "npm run install-client && npm run install-server && cd client && npm run build",
    "start": "cd server && node index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^9.2.1"
  },
  "engines": {
    "node": ">=20.0.0 <23.0.0"
  }
}
